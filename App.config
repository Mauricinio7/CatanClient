<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IGameEndPoint" />
                <binding name="BasicHttpBinding_IAccountEndPoint" />
            </basicHttpBinding>
            <netTcpBinding>
                <binding name="NetTcpBinding_IGameEndPoint">
                    <security mode="None">
                        <transport sslProtocols="None" />
                    </security>
                </binding>
                <binding name="NetTcpBinding_IAccountEndPoint">
                    <security mode="None">
                        <transport sslProtocols="None" />
                    </security>
                </binding>
            </netTcpBinding>
        </bindings>
        <client>
            <endpoint address="http://192.168.1.127:8191/GameService" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IGameEndPoint" contract="GameService.IGameEndPoint"
                name="BasicHttpBinding_IGameEndPoint" />
            <endpoint address="net.tcp://192.168.1.127:9192/GameService"
                binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IGameEndPoint"
                contract="GameService.IGameEndPoint" name="NetTcpBinding_IGameEndPoint" />
            <endpoint address="http://192.168.1.127:8181/AccountService"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IAccountEndPoint"
                contract="AccountService.IAccountEndPoint" name="BasicHttpBinding_IAccountEndPoint" />
            <endpoint address="net.tcp://192.168.1.127:8182/AccountService"
                binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IAccountEndPoint"
                contract="AccountService.IAccountEndPoint" name="NetTcpBinding_IAccountEndPoint" />
        </client>
    </system.serviceModel>
</configuration>